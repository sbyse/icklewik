@using Icklewik.Core.Model

@model Icklekwik.Server.ViewModels.Wiki.SiteModel


﻿@if (!Model.IsPartialView)
 {
     Layout = "_Layout.cshtml";
 }

<h1>Hello SITE!</h1>

<ul>
@foreach (ImmutableWikiEntry wikiEntry in Model.SiteMap)
{
    <li><a href="@wikiEntry.WikiUrl">@wikiEntry.WikiPath</a></li>
}
</ul>
